  "version": "0.0.1c",
  "custom": {
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/grp-16-session-template-validation:0.0.1c"
    },
    "flywheel": {
      "suite": "Curation"
    }
  },
  "inputs": {
    "api-key": {
